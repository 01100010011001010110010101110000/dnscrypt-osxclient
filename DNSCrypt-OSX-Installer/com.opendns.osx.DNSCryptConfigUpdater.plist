<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.opendns.osx.DNSCryptConfigUpdater</string>
	<key>KeepAlive</key>
	<true/>
	<key>RunAtLoad</key>
	<true/>
	<key>ProgramArguments</key>
